module Main;

let xs : new [List Byte](6, 3).init();

let b : byte('A');
let counter : byte(4);

while counter:
    ++xs;
    *xs = b;
    ++b;
    --counter;
end

print_list(xs);

while ++(xs#0):
    print(cast *(xs#0) : BChar);
end

println();

print_list(xs);

do
    print(cast *(xs#0) : BChar);
loop while --(xs#0);

println();

print_list(xs);


